{
  "name": "nexasoft-progresive-web-app",
  "description": "NexaSoft professional solutions, el futuro en soluciones de software.",
  "keywords": [
    "software development",
    "professional solutions",
    "web pages",
    "web sites",
    "mobile apps"
  ],
  "type": "module",
  "author": "Nexasoft SAS",
  "license": "MIT",
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "trustedDependencies": [
    "sharp"
  ],
  "trustedDependencies-annotation": "Needed for bun to allow running install scripts",
  "packageManager": "pnpm@9.13.0",
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/netlify-edge/vite.config.ts",
    "buildserver2": "vite build -c adapters/static/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy2": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "deploy": "netlify deploy --build",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "prefmt": "pnpm exec prettier . --write",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.10.2",
    "@builder.io/qwik": "^1.11.0",
    "@builder.io/qwik-city": "^1.11.0",
    "@builder.io/qwik-react": "0.5.5",
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@eslint/js": "^9.15.0",
    "@modular-forms/qwik": "^0.29.0",
    "@mui/icons-material": "^6.1.9",
    "@mui/material": "^6.1.9",
    "@mui/x-data-grid": "^7.22.3",
    "@netlify/edge-functions": "^2.11.1",
    "@supabase/supabase-js": "^2.46.1",
    "@types/eslint": "^9.6.1",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.10.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-swipeable-views": "^0.13.5",
    "@types/react-swipeable-views-utils": "^0.13.7",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "autoprefixer": "^10.4.20",
    "dotenv": "^16.4.5",
    "eslint": "^9.15.0",
    "eslint-plugin-qwik": "^1.11.0",
    "eslint-plugin-react": "^7.37.2",
    "globals": "^15.12.0",
    "netlify-cli": "^17.37.2",
    "postcss": "^8.4.49",
    "prettier": "^3.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-swipeable-views": "^0.14.0",
    "react-swipeable-views-utils": "^0.14.0",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0",
    "undici": "^7.0.0",
    "uuid": "^11.0.3",
    "vite": "^6.0.1",
    "vite-tsconfig-paths": "^5.1.3"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "mongodb": "^6.11.0",
    "mongoose": "^8.8.3",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.5"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  }
}
