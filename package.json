{
  "name": "nexasoft-progresive-web-app",
  "description": "NexaSoft professional solutions, el futuro en soluciones de software.",
  "keywords": [
    "software development",
    "professional solutions",
    "web pages",
    "web sites",
    "mobile apps"
  ],
  "author": "Nexasoft SAS",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/static/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.8.0",
    "@builder.io/qwik": "^1.2.6",
    "@builder.io/qwik-city": "^1.2.6",
    "@builder.io/qwik-react": "0.5.0",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@mui/material": "^5.14.1",
    "@mui/x-data-grid": "6.10.1",
    "@types/eslint": "8.44.0",
    "@types/node": "^20.4.4",
    "@types/react": "^18.2.16",
    "@types/react-dom": "^18.2.7",
    "@types/react-swipeable-views": "^0.13.2",
    "@types/react-swipeable-views-utils": "^0.13.4",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "autoprefixer": "^10.4.14",
    "dotenv": "^16.3.1",
    "eslint": "8.45.0",
    "eslint-plugin-qwik": "^1.2.6",
    "postcss": "^8.4.27",
    "prettier": "3.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-swipeable-views": "^0.14.0",
    "react-swipeable-views-utils": "^0.14.0",
    "tailwindcss": "^3.3.3",
    "typescript": "5.1.6",
    "undici": "5.22.1",
    "vite": "4.4.7",
    "vite-tsconfig-paths": "4.2.0"
  },
  "dependencies": {
    "@mui/icons-material": "^5.14.1",
    "mongodb": "^5.7.0",
    "mongoose": "^7.4.1",
    "rxjs": "^7.8.1"
  }
}
