{
  "name": "nexasoft-progresive-web-app",
  "description": "NexaSoft professional solutions, el futuro en soluciones de software.",
  "keywords": [
    "software development",
    "professional solutions",
    "web pages",
    "web sites",
    "mobile apps"
  ],
  "author": "Nexasoft SAS",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/static/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "prefmt": "pnpm exec prettier . --write",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.8.1",
    "@builder.io/qwik": "^1.2.16",
    "@builder.io/qwik-city": "^1.2.16",
    "@builder.io/qwik-react": "0.5.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.14.16",
    "@mui/x-data-grid": "^6.17.0",
    "@types/eslint": "^8.44.6",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.34",
    "@types/react-dom": "^18.2.14",
    "@types/react-swipeable-views": "^0.13.4",
    "@types/react-swipeable-views-utils": "^0.13.6",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "autoprefixer": "^10.4.16",
    "dotenv": "^16.3.1",
    "eslint": "^8.52.0",
    "eslint-plugin-qwik": "^1.2.16",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-swipeable-views": "^0.14.0",
    "react-swipeable-views-utils": "^0.14.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "undici": "^5.27.0",
    "vite": "^4.5.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "dependencies": {
    "@modular-forms/qwik": "^0.21.0",
    "@mui/icons-material": "^5.14.16",
    "@supabase/supabase-js": "^2.38.4",
    "clsx": "^2.0.0",
    "mongodb": "^6.2.0",
    "mongoose": "^7.6.4",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.1"
  }
}
