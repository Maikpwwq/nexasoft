{
  "name": "nexasoft-progresive-web-app",
  "description": "NexaSoft professional solutions, el futuro en soluciones de software.",
  "keywords": [
    "software development",
    "professional solutions",
    "web pages",
    "web sites",
    "mobile apps"
  ],
  "type": "module",
  "author": "Nexasoft SAS",
  "license": "MIT",
  "engines": {
    "node": ">=24.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "trustedDependencies": [
    "sharp"
  ],
  "trustedDependencies-annotation": "Needed for bun to allow running install scripts",
  "packageManager": "pnpm@9.13.0",
  "private": true,
  "scripts": {
    "build": "pnpm run build.client && pnpm run build.server",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/static/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "buildserver2": "vite build -c adapters/static/vite.config.ts",
    "deploy": "netlify deploy --build",
    "deploy2": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "prefmt": "pnpm exec prettier . --write",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.18.0",
    "@builder.io/qwik-city": "^1.18.0",
    "@builder.io/qwik-react": "0.5.7",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@eslint/js": "^9.39.2",
    "@modular-forms/qwik": "^0.29.1",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "@netlify/edge-functions": "^3.0.3",
    "@qwik.dev/partytown": "^0.11.2",
    "@supabase/supabase-js": "^2.89.0",
    "@tailwindcss/vite": "^4.1.18",
    "@types/eslint": "^9.6.1",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-swipeable-views": "^0.13.6",
    "@types/react-swipeable-views-utils": "^0.13.7",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "autoprefixer": "^10.4.23",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-plugin-qwik": "^1.18.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.5.0",
    "netlify-cli": "^23.13.0",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-swipeable-views": "^0.14.1",
    "react-swipeable-views-utils": "^0.14.1",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0",
    "undici": "^7.16.0",
    "vite": "^7.3.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "mongodb": "^7.0.0",
    "mongoose": "^9.0.2",
    "rxjs": "^7.8.2",
    "sharp": "^0.34.5"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  }
}
