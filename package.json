{
  "name": "nexasoft-progresive-web-app",
  "description": "NexaSoft professional solutions, el futuro en soluciones de software.",
  "keywords": [
    "software development",
    "professional solutions",
    "web pages",
    "web sites",
    "mobile apps"
  ],
  "type": "module",
  "author": "Nexasoft SAS",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/static/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "prefmt": "pnpm exec prettier . --write",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.9.1",
    "@builder.io/qwik": "^1.4.4",
    "@builder.io/qwik-city": "^1.4.4",
    "@builder.io/qwik-react": "0.5.3",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.15.10",
    "@mui/x-data-grid": "^6.19.4",
    "@types/eslint": "^8.56.2",
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/react-swipeable-views": "^0.13.5",
    "@types/react-swipeable-views-utils": "^0.13.7",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.17",
    "dotenv": "^16.4.2",
    "eslint": "^8.56.0",
    "eslint-plugin-qwik": "^1.4.4",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-swipeable-views": "^0.14.0",
    "react-swipeable-views-utils": "^0.14.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "undici": "^5.28.3",
    "vite": "^5.1.1",
    "vite-tsconfig-paths": "^4.3.1"
  },
  "dependencies": {
    "@modular-forms/qwik": "^0.23.1",
    "@mui/icons-material": "^5.15.10",
    "@supabase/supabase-js": "^2.39.3",
    "clsx": "^2.1.0",
    "mongodb": "^6.3.0",
    "mongoose": "^8.1.2",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.2",
    "uuid": "^9.0.1"
  }
}
