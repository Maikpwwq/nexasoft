{
  "name": "nexasoft-progresive-web-app",
  "version": "1.0.0",
  "description": "NexaSoft professional solutions, el futuro en soluciones de software.",
  "keywords": [
    "software development",
    "professional solutions",
    "web pages",
    "web sites",
    "mobile apps"
  ],
  "author": "Nexasoft",
  "license": "MIT",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.expressserver": "vite build -c adaptors/express/vite.config.ts",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/static/vite.config.ts",
    "build.staticserver": "vite build -c adaptors/static/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "build.viteserver": "vite build -c adaptors/netlify-edge/vite.config.ts",
    "deploy": "netlify deploy --build",
    "deploy.netlify": "netlify deploy",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "start:server": "vite --open --mode ssr functions:serve edge-inspect",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "0.104.0",
    "@builder.io/qwik-city": "0.104.0",
    "@builder.io/qwik-react": "0.5.0",
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@mui/material": "5.10.10",
    "@mui/x-data-grid": "6.2.1",
    "@netlify/edge-functions": "^2.0.0",
    "@netlify/functions": "^1.4.0",
    "@types/compression": "^1.7.2",
    "@types/eslint": "8.37.0",
    "@types/express": "4.17.17",
    "@types/node": "^18.16.2",
    "@types/node-fetch": "^2.6.3",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.1",
    "@typescript-eslint/eslint-plugin": "5.59.1",
    "@typescript-eslint/parser": "5.59.1",
    "autoprefixer": "10.4.14",
    "compression": "^1.7.4",
    "eslint": "8.39.0",
    "eslint-plugin-qwik": "0.104.0",
    "express": "4.18.2",
    "netlify-cli": "^14.3.1",
    "node-fetch": "3.3.1",
    "postcss": "^8.4.23",
    "prettier": "2.8.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "^3.3.2",
    "typescript": "5.0.4",
    "undici": "5.22.0",
    "vite": "4.3.3",
    "vite-tsconfig-paths": "4.2.0"
  },
  "dependencies": {
    "@babel/core": "^7.21.4",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.2",
    "@mui/system": "^5.12.1",
    "@types/react-swipeable-views": "^0.13.1",
    "@types/react-swipeable-views-utils": "^0.13.4",
    "dotenv": "^16.0.3",
    "mongodb": "^5.3.0",
    "mongoose": "^7.1.0",
    "react-swipeable-views": "^0.14.0",
    "react-swipeable-views-utils": "^0.14.0"
  },
  "peerDependencies": {
    "@babel/core": ">=7.0.0 || <8.0.0",
    "@mui/system": "^5.4.1",
    "react": ">=17.0.0 <18.0.0 || >=18.0.0 <19.0.0",
    "react-dom": ">=17.0.0 <18.0.0 || >=18.0.0 <19.0.0"
  }
}
